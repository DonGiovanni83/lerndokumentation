{
  "name": "barber-jekyll",
  "description": "Barber is a blog theme for Jekyll built by Thomas Vaeth at Samesies using HTML, Sass, and JavaScript.",
  "demo": "http://barber.samesies.io",
  "version": "1.0.0",
  "scripts": {
    "start": "bundle exec jekyll serve --config _config.yml _config.dev.yml",
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "deploy": "gulp build && bundle exec jekyll build --config _config.yml _config.prod.yml && s3-website deploy _site",
    "gulp": "gulp"
  },
  "author": {
    "name": "Samesies",
    "email": "okay@samesies.io",
    "url": "http://samesies.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samesies/barber-jekyll.git"
  },
  "bugs": "https://github.com/samesies/barber-jekyll/issues",
  "keywords": [
    "barber",
    "jekyll",
    "blog",
    "theme"
  ],
  "dependencies": {
    "aos": "^2.3.4",
    "jquery": "^3.5.1",
    "salvattore": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "eslint": "^7.15.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.0.2",
    "s3-website": "^3.3.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "license": "MIT"
}
